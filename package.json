    "grunt": "^0.4.5"
  },
  "dependencies": {
    "semver": "^4.1.0",
    "fs-extra": "0.12.0",
    "fs-promise": "0.2.0",
    "htmltidy": "0.0.6",
    "teacup": "0.4.2",
    "uberscore": "0.0.17",
    "underscore.string": "^2.3.3",
    "urequire-ab-grunt-contrib-watch": "0.0.x",
    "upath": "0.1.5",
    "when": "^3.6.2"
