{
  "name": "urequire-ab-specrunner",
  "description": "Automagically generates & runs mocha/chai/phantomjs tests from a uRequire `lib` & `specs` build.",
  "version": "0.2.1",
  "homepage": "https://github.com/anodynos/urequire-ab-specrunner",
  "author": {
    "name": "Agelos Pikoulas",
    "email": "agelos.pikoulas@gmail.com"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "http://www.opensource.org/licenses/mit-license.php"
    }
  ],
  "keywords": [
    "urequire",
    "specs",
    "test",
    "runner",
    "mocha",
    "chai",
    "phantomjs",
    "mocha-phantomjs",
    "grunt",
    "spec runner"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/anodynos/urequire-ab-specrunner"
  },
  "bugs": {
    "url": "git://github.com/anodynos/urequire-ab-specrunner/issues",
    "email": "agelos.pikoulas@gmail.com"
  },
  "main": "./build/code/urequire-ab-specrunner.js",
  "engines": {
    "node": ">=0.10"
